
#include <iostream>
#include <climits>
#include <bits/stdc++.h> 
using namespace std;

int max(int arr[],int n,int k){
    int m =n-k;
    int ans = arr[m];
    return ans;
}


int mini(int arr[],int n,int k){
    int m = k-1;
    int ans = arr[m];
    return ans;
}

int main()
{
    int arr[] = {4,5,6,8,4,3,1,3,11,90};
    int n = sizeof(arr)/sizeof(arr[0]);
    sort(arr,arr+n);
    int k;
    cout<<"Enter Kth number less than "<<n << " ";
    cin>>k;
    cout<<max(arr,n,k);
    

    return 0;
}
