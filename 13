
#include <iostream>

using namespace std;

int main()
{
     int arr[] = { 1, 2, 3, 4, 5, 6, 7, 7, 8 };
     int n = sizeof(arr)/sizeof(arr[0]);
     
     int m = n-1;
     int sum1 = m*(m+1)/2;
     
     int sum2 = 0;
     for(int i = 0; i<n;i++)
     {
         sum2 = sum2 + arr[i];
     }
    cout<<"The repeated no. is "<<sum2-sum1;
    return 0;
}
