
#include <iostream>

using namespace std;

bool
search (int matrix[3][4], int target)
{

  for (int i = 0; i < 3; i++)
    {
      for (int j = 0; j < 4; j++)
	{
	  if (matrix[i][j] == target)
	    {
	      return true;
	    }
	}
    }
  return false;
}



int
main ()
{
  int matrix[3][4] = { {1, 3, 5, 7}, {10, 11, 16, 20}, {23, 30, 34, 60} };
  int target = 3;

  cout << search (matrix, size, target);




  return 0;
}
